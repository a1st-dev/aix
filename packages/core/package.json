{
   "name": "@a1st/aix-core",
   "version": "0.0.0",
   "description": "Core utilities for aix configuration loading and management",
   "files": [
      "dist"
   ],
   "type": "module",
   "main": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "exports": {
      ".": {
         "types": "./dist/index.d.ts",
         "import": "./dist/index.js"
      }
   },
   "publishConfig": {
      "access": "public",
      "registry": "https://registry.npmjs.org/"
   },
   "scripts": {
      "build": "tsc -p tsconfig.build.json",
      "test": "vitest run",
      "test:watch": "vitest"
   },
   "dependencies": {
      "@a1st/aix-schema": "*",
      "c12": "3.3.3",
      "defu": "6.1.4",
      "giget": "2.0.0",
      "nypm": "0.6.2",
      "p-map": "7.0.4",
      "pathe": "2.0.3",
      "yaml": "2.8.2"
   }
}
