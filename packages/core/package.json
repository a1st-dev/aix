{
   "name": "@a1st/aix-core",
   "version": "0.0.10",
   "description": "Core utilities for aix configuration loading and management",
   "keywords": [
      "ai-agent",
      "ai-config",
      "config-loader",
      "mcp",
      "model-context-protocol"
   ],
   "repository": {
      "type": "git",
      "url": "https://github.com/a1st-dev/aix.git",
      "directory": "packages/core"
   },
   "files": [
      "dist"
   ],
   "type": "module",
   "main": "./dist/index.js",
   "types": "./dist/index.d.ts",
   "exports": {
      ".": {
         "types": "./dist/index.d.ts",
         "import": "./dist/index.js"
      }
   },
   "publishConfig": {
      "access": "public",
      "registry": "https://registry.npmjs.org/"
   },
   "scripts": {
      "build": "tsc -p tsconfig.build.json",
      "test": "vitest run",
      "test:watch": "vitest"
   },
   "dependencies": {
      "@a1st/aix-schema": "*",
      "c12": "3.3.3",
      "confbox": "0.2.4",
      "defu": "6.1.4",
      "detect-indent": "7.0.2",
      "giget": "3.1.2",
      "is-in-ci": "2.0.0",
      "nypm": "0.6.5",
      "p-map": "7.0.4",
      "pathe": "2.0.3",
      "yaml": "2.8.2"
   }
}
