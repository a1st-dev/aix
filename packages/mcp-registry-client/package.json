      "registry": "https://registry.npmjs.org/"
   },
   "scripts": {
      "build": "tsc -p tsconfig.build.json",
      "test": "vitest run --passWithNoTests",
      "test:watch": "vitest"
   },
   "dependencies": {
      "p-map": "7.0.4"
   }
}
